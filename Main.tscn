extends Node

const SVGRect = preload(\"res://addons/svg_data/svg_rect.gd\");

export var rect : Resource;
var svg_data;

func _ready():
	if not rect:
		rect = SVGRect.new();
	rect.id = self.name;
	svg_data = get_parent().svg;
	svg_data.add_element(rect);
"

